{
  "name": "season-master-strapi",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "npm-run-all clean rebuild:swc rebuild:rollup rebuild:sharp strapi-build",
    "clean": "rimraf .cache build dist",
    "rebuild:swc": "npm rebuild @swc/core --build-from-source --platform=linux --arch=x64 --libc=musl",
    "rebuild:rollup": "npm rebuild @rollup/rollup-linux-x64-musl --build-from-source",
    "rebuild:sharp": "npm rebuild sharp --build-from-source --platform=linux --arch=x64 --libc=musl",
    "strapi-build": "strapi build --clean",
    "console": "strapi console",
    "deploy": "strapi deploy",
    "dev": "strapi develop",
    "develop": "strapi develop",
    "start": "strapi start",
    "strapi": "strapi",
    "prebuild": "node -v && npm -v",
    "postinstall": "npm rebuild @swc/core @rollup/rollup-linux-x64-musl sharp --build-from-source --platform=linux --arch=x64 --libc=musl"
  },
  "dependencies": {
    "@strapi/plugin-cloud": "5.27.0",
    "@strapi/plugin-users-permissions": "5.27.0",
    "@strapi/provider-upload-cloudinary": "^5.27.0",
    "@strapi/strapi": "5.27.0",
    "better-sqlite3": "11.3.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5"
  },
  "engines": {
    "node": "20.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "c81c78cc-db95-460d-b8bf-dd09f1eef90b",
    "installId": "065319b16ecd5f33f6b97b40041b37fd90baeaefa519acfd0014e66ed1c6d598"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-musl": "^4.24.0",
    "@swc/core": "1.7.26",
    "sharp": "^0.33.5"
  }
}
